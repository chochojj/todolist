{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["react-app", "prettier"],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error", { "endOfLine": "auto" }] ,
      "react/react-in-jsx-scope": 0,
      "react/jsx-uses-react": 0
    }
  }